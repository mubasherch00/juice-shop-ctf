{
  "name": "juice-shop-ctf-cli",
  "version": "6.0.1",
  "description": "Capture-the-Flag (CTF) environment setup tools for OWASP Juice Shop",
  "author": "Bjoern Kimminich <bjoern.kimminich@owasp.org> (https://www.owasp.org/index.php/User:Bjoern_Kimminich)",
  "contributors": [
    "Bjoern Kimminich",
    "Josh Grossman"
  ],
  "keywords": [
    "web security",
    "web application security",
    "webappsec",
    "owasp",
    "pentest",
    "pentesting",
    "security",
    "vulnerable",
    "vulnerability",
    "broken",
    "capture the flag",
    "ctf",
    "ctfd",
    "fbctf",
    "cli"
  ],
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bkimminich/juice-shop-ctf.git"
  },
  "bugs": {
    "url": "https://github.com/bkimminich/juice-shop-ctf/issues"
  },
  "homepage": "http://owasp-juice.shop",
  "license": "MIT",
  "main": "index.js",
  "bin": {
    "juice-shop-ctf": "bin/juice-shop-ctf.js"
  },
  "scripts": {
    "lint": "standard",
    "fix": "standard --fix",
    "test": "nyc --reporter=html --reporter=text mocha test/unit",
    "e2e": "nyc --reporter=text mocha test/e2e --slow=10000",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "engines": {
    "node": "8 - 11"
  },
  "dependencies": {
    "bcryptjs": "~2.4.3",
    "bluebird": "~3.5.3",
    "colors": "~1.3.2",
    "yargs": "~13.2.2",
    "dateformat": "~3.0.3",
    "inquirer": "~6.2.0",
    "joi": "~14.3.1",
    "jssha": "~2.3.1",
    "js-yaml": "~3.13.0",
    "node-zip": "~1.1.1",
    "path": "~0.12.7",
    "request": "~2.88.0",
    "request-promise": "~4.2.2"
  },
  "devDependencies": {
    "ava": "~1.3.0",
    "chai": "~4.2.0",
    "chai-as-promised": "~7.1.1",
    "chai-spies": "~1.0.0",
    "chai-subset": "~1.6.0",
    "chai-things": "~0.2.0",
    "coveralls": "~3.0.2",
    "inquirer-test": "~2.0.1",
    "lockfile": "~1.0.4",
    "mocha": "~6.1.4",
    "nyc": "~13.3.0",
    "rewire": "~4.0.1",
    "standard": "~12.0.1"
  },
  "standard": {
    "env": [
      "node",
      "mocha",
      "chai"
    ],
    "globals": [
      "it",
      "describe",
      "beforeEach"
    ]
  },
  "nyc": {
    "report-dir": "./build/reports/coverage"
  }
}
